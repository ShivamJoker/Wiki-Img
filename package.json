{
  "name": "wiki-img",
  "type": "module",
  "version": "1.0.1",
  "description": "Easily search & download wikipedia images (Zero dependency lib)",
  "main": "lib/index.cjs",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "wikiimg": "lib/cli.js",
    "wiki-img": "lib/cli.js"
  },
  "exports": {
    ".": "lib/index.cjs",
    "mjs": "lib/index.js",
    "lib": "src/index.ts"
  },
  "scripts": {
    "build": "tsx scripts/build.ts",
    "build-docs": "tsx scripts/build-docs.ts",
    "prepublishOnly": "concurrently \"npm run build\" \"npm run build-docs\""
  },
  "keywords": [
    "wiki",
    "image-downloader",
    "node",
    "wikipedia",
    "scrapper"
  ],
  "author": "Shivam <hi@creativeshi.com>",
  "license": "AGPL3",
  "devDependencies": {
    "@swc/core": "^1.2.246",
    "@types/node": "^18.7.14",
    "concat-md": "^0.5.0",
    "concurrently": "^7.3.0",
    "npm-dts": "^1.3.12",
    "tsx": "^3.9.0",
    "typedoc": "0.23.13",
    "typedoc-plugin-markdown": "3.13.5",
    "typescript": "^4.8.2"
  }
}
